{
  "name": "vue2.7-ts-webpack",
  "version": "1.0.0",
  "description": "vue2.7.14 ts webpack5",
  "author": "wangguixing",
  "license": "MIT",
  "scripts": {
    "re-install": "rm -rf node_modules && npm install --legacy-peer-deps",
    "increase-memory-limit": "increase-memory-limit",
    "dev": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --config build/webpack.dev.js",
    "build:prod": "npm run clean && webpack --config build/webpack.prod.js",
    "build:test": "npm run clean && webpack --config build/webpack.prod.js",
    "clean": "rm -rf dist",
    "analyzer": "webpack --config build/webpack.analyzer.js",
    "lint": "eslint --ext .js,.vue src",
    "release-tag": "standard-version --release-as v0.1.0-alpha && git push --follow-tags",
    "release": "standard-version --release-as v0.1.0-alpha --infile=changelog.md --skip.tag=true && git push"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "ts",
    "webpack5"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
